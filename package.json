{
  "name": "create-content-loader",
  "homepage": "https://danilowoz.github.io/create-content-loader",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
